    "@types/node": "^22.5.4",
    "@vitest/coverage-v8": "^2.0.5",
    "husky": "^9.1.6",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
