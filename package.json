{
  "name": "nodejs-api-authentication",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "lint": "eslint . --ext .ts",
    "vitest": "vitest --globals --config vitest.config.ts",
    "test": "npm run vitest -- run",
    "test:unit": "npm run vitest -- --project unit --run",
    "test:integration": "npm run vitest -- --project integration --run",
    "test:watch": "npm run vitest",
    "test:coverage": "npm run vitest -- run --coverage",
    "test:ci": "npm run vitest -- run --silent --coverage --reporter=junit --outputFile.junit=./test-output/junit.xml",
    "prepare": "husky",
    "build": "tsup",
    "dev": "tsx watch src/server.ts"
  },
  "keywords": [],
  "author": "https://github.com/ribeirogab",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/lib-dynamodb": "^3.654.0",
    "@aws-sdk/util-dynamodb": "^3.654.0",
    "@fastify/aws-lambda": "^4.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "env-var": "^7.5.0",
    "fastify": "^4.28.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.0.0",
    "tsyringe": "^4.8.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ribeirogab/eslint-config": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "dotenv": "^16.4.5",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "4.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  }
}
